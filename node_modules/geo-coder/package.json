{
  "name": "geo-coder",
  "version": "1.0.2",
  "description": "address lookup/autocomplete/reverse-geolookup for Google, OpenStreet, and Bing",
  "main": "dist/geoCode.umd.min.js",
  "module": "src/geo-code.js",
  "scripts": {
    "webpack": "webpack",
    "clean": "rm -rf dist && mkdir dist",
    "min-js": "uglifyjs dist/geoCode.umd.js -c > dist/geoCode.umd.min.js",
    "build": "npm-run-all clean webpack min-js",
    "doc": "jsdoc2md src/**/*.js > DOC-GENERATED.md",
    "build-and-publish": "npm run build && npm version patch && npm publish --access=public",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenhwkim/geo-code.git"
  },
  "bugs": {
    "url": "https://github.com/allenhwkim/geo-code/issues"
  },
  "homepage": "https://github.com/allenhwkim/geo-code#readme",
  "keywords": [
    "google",
    "address",
    "geocode",
    "bing",
    "OpenStreet",
    "map"
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "jest": "^22.0.3",
    "jsdoc-to-markdown": "3.0.2",
    "node-fetch": "^1.7.3",
    "npm-run-all": "^4.1.1",
    "regenerator-runtime": "^0.11.1",
    "uglify-es": "3.1.7",
    "uglify-js": "^3.2.2",
    "uglifycss": "0.0.27",
    "webpack": "^3.8.1"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-es2016",
      "babel-preset-es2017"
    ]
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "dependencies": {
    "node-fetch": "^1.7.3"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "license": "MIT"
}
